#
# Chef Client Config File
#
# Dynamically generated by Chef - local modifications will be replaced
#

log_level          :info
log_location       <%= @client_log %>
ssl_verify_mode    :verify_none
chef_server_url    "<%= @node[:chef][:url_type] %>://<%= @node[:chef][:server_fqdn] %>:<%= @node[:chef][:server_port] %>"

validation_client_name "validator"
validation_key      	 "/etc/chef/validation.pem"
client_key          	 "/etc/chef/client.pem"

file_cache_path    "<%= @node[:chef][:cache_path] %>"
pid_file           "<%= @node[:chef][:run_path] %>/client.pid"

Mixlib::Log::Formatter.show_time = <%= @show_time %>
