#
# Chef Server Config File
#
# Dynamically generated by Chef - local modifications will be replaced
#

log_level          :info
log_location       "<%= @node[:chef][:client_log] %>"
ssl_verify_mode    :verify_none
registration_url   "https://chef.<%= @node[:domain]%>"
openid_url         "https://chef.<%= @node[:domain]%>"
template_url       "https://chef.<%= @node[:domain]%>"
remotefile_url     "https://chef.<%= @node[:domain]%>"
search_url         "https://chef.<%= @node[:domain]%>"

file_store_path    "<%= @node[:chef][:path] %>/file_store"
file_cache_path    "<%= @node[:chef][:path] %>/cache"

Chef::Log::Formatter.show_time = true
